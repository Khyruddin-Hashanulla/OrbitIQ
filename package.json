{
  "name": "orbitiq",
  "version": "1.0.0",
  "description": "Space Data & Missions Hub - Your one-stop platform for space exploration",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "build:production": "cd server && npm ci --only=production",
    "start": "cd server && npm start",
    "install-deps": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": ["space", "satellites", "missions", "astronomy", "tracking"],
  "author": "OrbitIQ Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
